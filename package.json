{
	"name": "whatsapp-group-manager-bot",
	"version": "1.0.0",
	"description": "WhatsApp Bot for Manijas en Noosa",
	"homepage": "https://github.com/baltasarsolanilla/whatsapp-group-manager-bot#readme",
	"bugs": {
		"url": "https://github.com/baltasarsolanilla/whatsapp-group-manager-bot/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/baltasarsolanilla/whatsapp-group-manager-bot.git"
	},
	"license": "MIT",
	"author": "Baltasar Solanilla",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"dev": "tsx watch -r tsconfig-paths/register src/index.ts",
		"build:ts": "tsc",
		"build:alias": "tsc-alias",
		"build": "npm run build:ts && npm run build:alias && tsc-esm-fix dist",
		"start": "node dist/index.js",
		"test": "jest --passWithNoTests",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"fix-all": "npm run format && npm run lint:fix",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@prisma/client": "^6.16.1",
		"axios": "^1.11.0",
		"dotenv": "^17.2.2",
		"express": "^5.1.0",
		"pg": "^8.16.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.35.0",
		"@types/express": "^5.0.3",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.4.0",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"jest": "^30.1.3",
		"prettier": "^3.6.2",
		"prisma": "^6.16.1",
		"ts-jest": "^29.4.1",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.16",
		"tsc-esm-fix": "^3.1.2",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.43.0"
	}
}
